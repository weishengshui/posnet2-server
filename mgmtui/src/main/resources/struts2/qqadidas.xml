<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
	"http://struts.apache.org/dtds/struts-2.1.7.dtd">

<struts>
	<package namespace="/qquery" name="search" extends="struts-default">
		<action name="search" class="com.chinarewards.qqgbvpn.mgmtui.struts.qq.adidas.QQMemberSearchAction" method="searchMemberObtainStatus">
			<result name="success">/WEB-INF/jsp/qqadidas/search.jsp</result>
			<result name="input">/WEB-INF/jsp/qqadidas/search.jsp</result>
			<interceptor-ref name="defaultStack" />
		</action>
		<action name="qqadidas" class="com.chinarewards.qqgbvpn.mgmtui.struts.qq.adidas.QQMemberSearchAction" method="execute">
			<result name="success">
				/WEB-INF/jsp/qqadidas/search.jsp
			</result>
		</action>
	</package>
</struts>